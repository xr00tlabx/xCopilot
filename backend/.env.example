# Exemplo de configuração para xCopilot (NÃO COLOQUE CHAVES REAIS NESTE ARQUIVO)
OPENAI_API_KEY=your_openai_api_key_here
PORT=3000

# Configurações do Elasticsearch
ELASTICSEARCH_NODE=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=your_elasticsearch_password_here
ELASTICSEARCH_REQUEST_TIMEOUT=60000
ELASTICSEARCH_PING_TIMEOUT=10000
ELASTICSEARCH_SNIFF_ON_START=false
ELASTICSEARCH_MAX_RETRIES=3
ELASTICSEARCH_TLS_REJECT_UNAUTHORIZED=false